<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.TextField?>
<VBox alignment="CENTER" prefHeight="402.0" prefWidth="672.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.csd_214_lab3_bibek_lamichhane.Admin">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <Label fx:id="nnn" text="Name" />
        <TextField fx:id="name"/>
        <Label fx:id="eee" text="Email" />
        <TextField fx:id="email"/>
        <Label fx:id="ppp" text="Phone" />
        <TextField fx:id="phone"/>
        <Button mnemonicParsing="false" onAction="#CreateData" text="Create" />
        <Button mnemonicParsing="false" onAction="#UpdateData" text="Update" />
        <Button mnemonicParsing="false" onAction="#DeleteData" text="Delete" />
        <Button mnemonicParsing="false" onAction="#ViewData" text="View" />
        <Button mnemonicParsing="false" onAction="#GoBack" text="Back" />
        <TableView prefHeight="200.0" prefWidth="200.0">
            <columns>
                <TableColumn fx:id="id" prefWidth="75.0" text="Id" />
                <TableColumn fx:id="namee" prefWidth="75.0" text="Name" />
                <TableColumn fx:id="emaill" prefWidth="75.0" text="Email" />
                <TableColumn fx:id="phonee" prefWidth="75.0" text="Phone" />
                <TableColumn fx:id="typee" prefWidth="75.0" text="Type" />
            </columns>
        </TableView>
    </children>
</VBox>
